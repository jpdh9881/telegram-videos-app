{
   "name": "telegram-videos-app",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "devDependencies": {
      "@types/node": "^16.11.10",
      "ts-node": "^10.7.0",
      "typescript": "4.5.2"
   },
   "dependencies": {
      "axios": "^1.6.0",
      "cron": "^3.1.6",
      "dotenv": "^16.3.1",
      "fastify": "^4.24.3",
      "input": "^1.0.1",
      "lodash": "^4.17.21",
      "log4js": "^6.9.1",
      "mysql": "^2.14.1",
      "node-fetch": "^3.3.2",
      "reflect-metadata": "^0.1.13",
      "telegram": "^2.19.8",
      "typeorm": "0.3.17"
   },
   "scripts": {
      "start": "ts-node src/index.ts",
      "typeorm": "typeorm-ts-node-esm",
      "scrape-hash1": "ts-node src/index.ts -job scrape-hash-1",
      "scrape-hash2": "ts-node src/index.ts -job scrape-hash-2",
      "hash": "ts-node src/index.ts -job just-hashes",
      "sync": "ts-node src/index.ts -job sync",
      "crons": "ts-node src/index.ts -job crons",
      "migrate": "npx typeorm-ts-node-esm migration:run --dataSource './build/src/data-source.js'"
   }
